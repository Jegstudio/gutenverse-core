{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"gutenverse-core/*": ["./src/*"],
            "gutenverse-core-editor/*": ["./src/*"],
            "gutenverse-core-editor/helper": ["./src/editor-helper"],
            "gutenverse-core-frontend/*": ["./src/*"],
            "gutenverse-core-frontend/blocks": ["./src/frontend-blocks"]
		}
	},
	"include": ["./src/**/*"],
	"exclude": ["node_modules", "build"]
}
