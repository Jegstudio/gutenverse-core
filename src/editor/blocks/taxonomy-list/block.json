{
	"apiVersion": 3,
	"name": "gutenverse/taxonomy-list",
	"title": "Taxonomy List",
	"description": "Create a list of categories",
	"class_callback": "\\Gutenverse\\Block\\Taxonomy_List",
	"category": "gutenverse-post",
	"allowCopyStyle": true,
	"attributes": {
		"elementId": {
			"type": "string"
		},
		"qty": {
			"type": "integer",
